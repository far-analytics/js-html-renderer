  "engines": {
    "npm": ">=10.2.3",
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@types/web": "^0.0.139",
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
